@model List<Tick_win.Models.TransitionalViewModel>

@for (int i = 0; i < Model.Count(); i++)
{


    <br>
    @Html.HiddenFor(modelItem => modelItem[i].StopStation, Model[i].StopStation)
    @Html.DisplayFor(modelItem => modelItem[i].StopStation)
    <br>
    @Html.TextBoxFor(modelItem => modelItem[i].Departure, Model[i].Departure)
    <br>
    @Html.TextBoxFor(modelItem => modelItem[i].Arrival, Model[i].Arrival)
    <br>
}